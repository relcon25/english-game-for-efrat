{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "html"
    ],
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "no-undef": "error"
    }
}